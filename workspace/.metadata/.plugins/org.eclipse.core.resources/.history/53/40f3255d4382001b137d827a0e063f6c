package quiz06;

public class Book {

	// field
	String title;
	String writer;
	int price;
	
	Book(String title, String writer, int price) {
		this.title = title;
		this.writer = writer;
		this.price = price;
	}
	
	Book(String title, int price) {
		this(title, "미상", price);
	}
	Book() {
		// this("없음", "미상", 0);
		this("없음", 0);
	}
	
	
	
}
